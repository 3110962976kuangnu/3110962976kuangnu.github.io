#include &lt;stdio.h&gt;&#13;
#include 'stdint.h'&#13;
#include &lt;stdarg.h&gt;&#13;
&#13;
int my_printf(const char *format, ...) {&#13;
    staitc char tmp_str[100] = {0};&#13;
    va_list args;&#13;
